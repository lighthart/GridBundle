<{{- cell.type }} {% for attrib, value in cell.attr -%}{{- attrib -}}="{{- value|trim -}}" {%- endfor -%} {% if cell.attr['width'] is defined %}style="width:{{cell.attr['width']}};"{% endif %}>
    {%- if cell.attr.checkbox is defined %}
        <input type="checkbox"/>
    {%- elseif cell.value -%}
        {%- if cell.value.timestamp is defined -%}
            {{- cell.value|date('Y-m-d') -}}
        {%- elseif cell.html is defined -%}
            {{- cell.value|raw -}}
        {%- else -%}
            {{- cell.value -}}
        {%- endif -%}
    {%- elseif 'th' == cell.type -%}
        {%- if cell.attr.filter is defined %}
            <input type="text" id="lg-grid-search-{{cell.attr['data-role-lg-class']}}_{{cell.attr['data-role-lg-field']}}" class="lg-grid-filter-input"/>
        {%- elseif cell.html is defined -%}
            {{- cell.title|raw -}}
        {% else %}
            {{- cell.title -}}
        {% endif %}
    {%- endif -%}
</{{- cell.type -}}>