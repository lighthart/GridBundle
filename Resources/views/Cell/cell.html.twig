<{{- cell.type }} {% for attrib, value in cell.attr -%}{{- attrib -}}="{{- value -}}" {%- endfor -%}>
    {%- if cell.attr.checkbox is defined %}
        <input type="checkbox"/>
    {%- elseif cell.value -%}
        {%- if cell.value.timestamp is defined -%}
            {{- cell.value|date('Y-m-d') -}}
        {%- else -%}
            {{- cell.value -}}
        {%- endif -%}
    {%- elseif 'th' == cell.type -%}
        {{- cell.title -}}
    {%- endif -%}
</{{- cell.type -}}>